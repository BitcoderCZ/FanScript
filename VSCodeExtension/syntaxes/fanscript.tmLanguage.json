{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "FanScript",
	"patterns": [
		{
			"include": "#keywords"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.fanscript",
				"match": "\\b(if|else|on|while|break|continue)\\b"
			}]
		}
	},
	"scopeName": "source.fancade"
}